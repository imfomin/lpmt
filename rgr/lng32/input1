;;;;;;;
X, Y, Z : int;
let X = Y = Z = (2+3) * 4 / 56 %(28-5);

P1, P2 : pol; 

LLL for I = 2*5 to 100 addding 25
		put(Y*X);
		Y = Y+ X;
next LLL

if (Z != 0) for goto x1x 



			else goto x2x    ;

load   (P1, P2);
put(P1,P2);

:     
x1x
     goto   LLL;
:    x2x

switch (X *P1)
 				case 1: put(1);;
 				case []: break;
 	default: let X = Y;

error   ;
